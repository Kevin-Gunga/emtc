{
    "version": 2,
    "builds": [
        {
            "src": "api/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index"
        },
        {
            "src": "/(.*)",
            "dest": "/admission"
        },
        {
            "src": "/(.*)",
            "dest": "/academics"
        }
    ]
}
